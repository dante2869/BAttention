@model Sln.Models.VResultado

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Edit</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>VResultado</h4>
        <hr />
        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.TRATAMIENTO)

        @Html.HiddenFor(model => model.ACTIVIDAD)

        <div class="form-group">
            @Html.LabelFor(model => model.PACIENTE, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.PACIENTE)
                @Html.ValidationMessageFor(model => model.PACIENTE)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.EDAD, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.EDAD)
                @Html.ValidationMessageFor(model => model.EDAD)
            </div>
        </div>

        @Html.HiddenFor(model => model.SEXO)

        @Html.HiddenFor(model => model.ACIERTOS)

        @Html.HiddenFor(model => model.ERRORES)

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
